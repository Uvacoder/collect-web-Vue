{
  "name": "awesome-web",
  "version": "1.2.0",
  "description": "A brand new repository for web development prototypes",
  "scripts": {
    "build": "npm run build -ws --if-present",
    "lint": "npm run lint -ws --if-present",
    "prelease": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "start": "npm run start:react-sandbox",
    "start:react-sandbox": "npm start -w @sabertazimi/react-sandbox",
    "test": "npm run test -ws --if-present"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/awesome-web.git"
  },
  "keywords": [
    "html",
    "html5",
    "css",
    "css3",
    "js",
    "javascript",
    "ecmascript",
    "react",
    "reactjs",
    "vue",
    "vuejs",
    "node",
    "nodejs",
    "front-end",
    "front-end development",
    "web",
    "web development"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/awesome-web/issues"
  },
  "homepage": "https://github.com/sabertazimi/awesome-web",
  "devDependencies": {
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "pm2": "^5.1.1",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "workspaces": [
    "./packages/*"
  ]
}
