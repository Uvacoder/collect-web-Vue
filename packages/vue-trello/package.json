{
  "name": "@sabertazimi/vue-trello",
  "version": "1.8.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && yarn build:post",
    "build:post": "mkdir -p ../../build/vue-trello && cp -fr ./dist/* ../../build/vue-trello",
    "dev": "vite",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts,.vue ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.11.0",
    "@dg-scripts/stylelint-config": "^5.11.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.26.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vue-tsc": "^1.0.8"
  }
}
