{
  "name": "@sabertazimi/vue-trello",
  "version": "1.8.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && pnpm build:post",
    "build:post": "mkdir -p ../../build/vue-trello && cp -fr ./dist/* ../../build/vue-trello",
    "dev": "vite",
    "lint": "pnpm lint:style && pnpm lint:type-check",
    "lint:style": "eslint --ext .ts,.vue ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "lint:fix": "eslint --fix ./src --ext .vue,.js,.ts,.jsx,.tsx,.json && stylelint ./src/**/*.{css,vue} --fix",
    "serve": "vite preview"
  },
  "dependencies": {
    "nanoid": "^5.0.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.14.3",
    "@dg-scripts/stylelint-config": "^5.14.3",
    "@types/node": "^20.11.25",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vue-tsc": "^2.0.6"
  }
}
