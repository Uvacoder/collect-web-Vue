{
  "name": "@sabertazimi/vue-trello",
  "version": "1.8.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && yarn build:post",
    "build:post": "mkdir -p ../../build/vue-trello && cp -fr ./dist/* ../../build/vue-trello",
    "dev": "vite",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts,.vue ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "nanoid": "^4.0.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.13.3",
    "@dg-scripts/stylelint-config": "^5.13.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue-tsc": "^1.1.7"
  }
}
