{
  "name": "@sabertazimi/vue-trello",
  "version": "1.8.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && yarn build:post",
    "build:post": "mkdir -p ../../build/vue-trello && cp -fr ./dist/* ../../build/vue-trello",
    "dev": "vite",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts,.vue ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "nanoid": "^5.0.4",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.14.1",
    "@dg-scripts/stylelint-config": "^5.14.1",
    "@vitejs/plugin-vue": "^5.0.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "stylelint": "^16.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.27"
  }
}
