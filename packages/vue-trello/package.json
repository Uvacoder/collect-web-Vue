{
  "name": "@sabertazimi/vue-trello",
  "version": "1.7.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && yarn build:post",
    "build:post": "mkdir -p ../../build/vue-trello && cp -fr ./dist/* ../../build/vue-trello",
    "dev": "vite",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts,.vue ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "nanoid": "^3.3.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.8.0",
    "@dg-scripts/stylelint-config": "^5.8.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.12.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "stylelint": "^14.6.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vue-tsc": "^0.33.9"
  }
}
