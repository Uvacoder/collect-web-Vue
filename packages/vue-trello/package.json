{
  "name": "@sabertazimi/vue-trello",
  "version": "1.6.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && yarn build:post",
    "build:post": "mkdir -p ../../build/vue-trello && cp -fr ./dist/* ../../build/vue-trello",
    "dev": "vite",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts,.vue ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "nanoid": "^3.1.29",
    "vue": "^3.2.20",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.5.0",
    "@dg-scripts/stylelint-config": "^5.5.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.3.7",
    "eslint": "^7.32.0",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3",
    "vite": "^2.6.5",
    "vue-tsc": "^0.28.1"
  }
}
