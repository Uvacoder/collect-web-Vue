{
  "name": "@sabertazimi/vue-trello",
  "version": "1.6.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && yarn build:post",
    "build:post": "mkdir -p ../../build/vue-trello && cp -fr ./dist/* ../../build/vue-trello",
    "dev": "vite",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts,.vue ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "nanoid": "^3.1.30",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.5.10",
    "@dg-scripts/stylelint-config": "^5.5.10",
    "@vitejs/plugin-vue": "^1.9.4",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.2.0",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vue-tsc": "^0.29.5"
  }
}
