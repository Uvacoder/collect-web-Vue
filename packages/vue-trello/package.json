{
  "name": "@sabertazimi/vue-trello",
  "version": "1.7.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && yarn build:post",
    "build:post": "mkdir -p ../../build/vue-trello && cp -fr ./dist/* ../../build/vue-trello",
    "dev": "vite",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts,.vue ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.10.0",
    "@dg-scripts/stylelint-config": "^5.10.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.19.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vue-tsc": "^0.38.5"
  }
}
