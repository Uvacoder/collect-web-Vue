{
  "name": "@sabertazimi/vue-trello",
  "version": "1.6.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && yarn build:post",
    "build:post": "mkdir -p ../../build/vue-trello && cp -fr ./dist/* ../../build/vue-trello",
    "dev": "vite",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts,.vue ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "nanoid": "^3.1.28",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^1.1.3",
    "@dg-scripts/stylelint-config": "^1.1.3",
    "@vitejs/plugin-vue": "^1.9.2",
    "autoprefixer": "^10.3.6",
    "eslint": "^7.32.0",
    "postcss": "^8.3.8",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vue-tsc": "^0.3.0"
  }
}
