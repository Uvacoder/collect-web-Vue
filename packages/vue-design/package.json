{
  "name": "@sabertazimi/vue-design",
  "private": true,
  "version": "1.8.0",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc && vite build && yarn build:post",
    "build:post": "mkdir -p ../../build/vue-design && cp -fr ./dist/* ../../build/vue-design",
    "dev": "vite",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "lint:style": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:type-check": "tsc --noEmit",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.14.2",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  },
  "dependencies": {
    "snabbdom": "^3.6.2"
  }
}
